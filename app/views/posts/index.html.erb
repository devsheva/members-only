<ul>
  <% @posts.each do |post| %>
    <li><strong>Title: </strong>: <%= post.title %></li>
    <li><strong>Body:</strong> <%= post.body %></li>
    <br>
    <% if member_signed_in? %>
      <p>Author name: <%= Member.find(post.member_id).email %> </p>
    <% end %>
  <% end %>


</ul>

<%= link_to "New post", new_post_path %>